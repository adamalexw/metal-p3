{
  "name": "album-ui",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/album/ui/src",
  "prefix": "app",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/album/ui/src/**/*.ts", "libs/album/ui/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/album/ui"],
      "options": {
        "jestConfig": "libs/album/ui/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:album", "tags:ui"]
}
