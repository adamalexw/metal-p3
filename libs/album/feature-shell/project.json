  "sourceRoot": "libs/album/feature-shell/src",
  "prefix": "app",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/album/feature-shell/src/**/*.ts", "libs/album/feature-shell/src/**/*.html"]
      }
