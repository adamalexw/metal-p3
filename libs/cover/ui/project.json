{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/cover/ui/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/cover/ui"
      ],
      "options": {
        "jestConfig": "libs/cover/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/cover/ui/src/**/*.ts",
          "libs/cover/ui/src/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "scope:cover",
    "type:ui"
  ]
}
