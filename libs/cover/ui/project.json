{
  "projectType": "library",
  "root": "libs/cover/ui",
  "sourceRoot": "libs/cover/ui/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cover/ui"],
      "options": {
        "jestConfig": "libs/cover/ui/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/cover/ui/src/**/*.ts", "libs/cover/ui/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:cover", "type:ui"]
}
