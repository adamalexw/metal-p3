{
  "projectType": "library",
  "root": "libs/shared/navigation",
  "sourceRoot": "libs/shared/navigation/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/navigation"],
      "options": {
        "jestConfig": "libs/shared/navigation/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/shared/navigation/src/**/*.ts", "libs/shared/navigation/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:shared", "type:ui"]
}
