    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/shared/feedback/src/**/*.ts", "libs/shared/feedback/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:shared", "type:util"]
}
