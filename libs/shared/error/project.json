{
  "projectType": "library",
  "sourceRoot": "libs/shared/error/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/error"],
      "options": {
        "jestConfig": "libs/shared/error/jest.config.ts",
        "passWithNoTests": true
