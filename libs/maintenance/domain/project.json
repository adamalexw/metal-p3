{
  "projectType": "library",
  "root": "libs/maintenance/domain",
  "sourceRoot": "libs/maintenance/domain/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/maintenance/domain"],
      "options": {
        "jestConfig": "libs/maintenance/domain/jest.config.js",
        "passWithNoTests": true
