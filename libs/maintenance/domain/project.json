    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/maintenance/domain/src/**/*.ts", "libs/maintenance/domain/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:maintenance", "type:domain"]
}
