{
  "name": "maintenance-ui",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/maintenance/ui/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/maintenance/ui"],
      "options": {
        "jestConfig": "libs/maintenance/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/maintenance/ui/src/**/*.ts", "libs/maintenance/ui/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:maintenance", "type:ui"]
}
