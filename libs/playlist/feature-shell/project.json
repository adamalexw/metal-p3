{
  "projectType": "library",
  "root": "libs/playlist/feature-shell",
  "sourceRoot": "libs/playlist/feature-shell/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/playlist/feature-shell"],
      "options": {
        "jestConfig": "libs/playlist/feature-shell/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/playlist/feature-shell/src/**/*.ts", "libs/playlist/feature-shell/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:playlist", "type:feature"]
}
